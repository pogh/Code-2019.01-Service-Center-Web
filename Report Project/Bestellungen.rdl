<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="MAINDB">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=base-server3;Initial Catalog=MAINDB</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>6f9e7e2a-3737-4b00-b082-338834988dc9</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet">
      <Query>
        <DataSourceName>MAINDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@reportDate">
            <Value>=Parameters!reportDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>USE MAINDB;

--DECLARE @reportDate DATE = CAST(GETUTCDATE() AS DATE)

SELECT 
	I.InvoicePk, I.InvoiceId, ISNULL(I.ChangedUTC, I.CreatedUTC) ChangedUTC, I.ChangedUserName, 
	ISNULL(BA.CustomerNumber, く.kunden_nr) CustomerNumber, LTRIM(RTRIM(ISNULL(BA.FirstName, '') + ' ' + ISNULL(BA.LastName, ''))) FullName, K.zahlungsweise, K.erledigt,
	B.beleg_nr belegnr, CASE WHEN I.InvoiceId &gt; 0 AND BA.CustomerNumber IS NULL THEN CAST(1 AS BIT) ELSE CAST(0 AS BIT) END IstNeukunde
FROM [ServiceCenter].[Invoice] I
JOIN [ServiceCenter].[InvoiceBillingAddress] BA ON I.InvoicePk = BA.InvoiceFk
LEFT JOIN [dbo].[einlesen_Kopfdaten] K ON I.InvoiceId = K.PKorder_id
LEFT JOIN [dbo].[belege] B ON K.bestellnr = B.ord_orderID
LEFT JOIN [dbo].[Kunden] く ON B.FKord_cus_id = く.PKkunden_id
WHERE (ISNULL(I.ChangedUTC, I.CreatedUTC) &gt;= @reportDate AND ISNULL(I.ChangedUTC, I.CreatedUTC) &lt; DATEADD(DAY, 1, @reportDate))
   OR I.InvoicePk IN (SELECT InvoiceFk FROM [ServiceCenter].[InvoiceItem] WHERE (ISNULL(ChangedUTC, CreatedUTC) &gt;= @reportDate AND ISNULL(ChangedUTC, CreatedUTC) &lt; DATEADD(DAY, 1, @reportDate)))
ORDER BY I.ChangedUTC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
